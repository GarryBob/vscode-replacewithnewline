{
	"name": "ReplaceWithNewline",
	"description": "search'n'replace including newlines and tabs in your open file from VSCode commandline",
	"version": "0.0.1",
	"publisher": "jmallien",
	"displayName": "ReplaceWithNewline",
	"keywords": [
		"find",
		"search",
		"replace",
		"return",
		"newline",
		"linefeed",
		"carrigereturn",
		"tab",
		"tabulator"
	],
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.replaceWithNewline"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.replaceWithNewline",
			"title": "search'n'replace with newline support"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	},
	"bugs": {
		"url": "https://github.com/jmallien/vscode-replacewithnewline/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jmallien/vscode-replacewithnewline.git"
	},
	"homepage": "https://github.com/jmallien/vscode-replacewithnewline/blob/master/README.md"
}