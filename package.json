{
	"name": "SNRWN",
	"description": "search'n'replace including newlines and tabs in your open file from VSCode commandline",
	"version": "0.0.1",
	"publisher": "Jens Mallien",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.searchNReplace"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.searchNReplace",
			"title": "search'n'replace with newline support"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}